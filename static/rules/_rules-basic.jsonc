      { "find": "Sword",       "replace": "Broadsword", "options": "ic", "filter": "OneHandSword" },
      { "find": "Broad Sword", "replace": "Broadsword", "options": "ic", "filter": "OneHandSword" },
      { "find": "Long Sword",  "replace": "Longsword",  "options": "ic", "filter": "OneHandSword" },
      { "find": "Bow",         "replace": "Shortbow",   "options": "ic", "filter": "Bow"          },
      { "find": "Short Bow",   "replace": "Shortbow",   "options": "ic", "filter": "Bow"          },
      { "find": "Long Bow",    "replace": "Longbow",    "options": "ic", "filter": "Bow"          }
    ],


